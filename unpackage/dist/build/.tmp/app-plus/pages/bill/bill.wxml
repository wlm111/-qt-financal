<view><view class="uni-tab-bar"><swiper class="swiper-box" style="{{'height:'+(swiperheight+'px')+';'}}" current="{{tabIndex}}"><swiper-item><scroll-view class="list" scroll-y data-event-opts="{{[['scrolltolower',[['loadmore']]]]}}" bindscrolltolower="__e"><view class="inpt"><input type="digit" placeholder="请输入金额" data-event-opts="{{[['input',[['__set_model',['','money','$event',[]]]]]]}}" value="{{money}}" bindinput="__e"/></view><view class="user-set-userinfo-list u-f-ac u-f-jsb"><view>记账类型</view><view data-event-opts="{{[['tap',[['changeOne',['type']]]]]}}" class="u-f-ac" bindtap="__e"><view>{{typeArr[type]}}</view><view class="icon iconfont icon-bi"></view></view></view><view class="user-set-userinfo-list u-f-ac u-f-jsb"><view>分类名称</view><picker mode="selector" range="{{category.range}}" data-event-opts="{{[['change',[['changeCategory',['$event']]]]]}}" bindchange="__e"><view class="u-f-ac"><view>{{category.title?category.title:"选择分类"}}</view><view class="icon iconfont icon-bi"></view></view></picker></view><view class="user-set-userinfo-list u-f-ac u-f-jsb"><view>账户</view><view data-event-opts="{{[['tap',[['changeOne',['account_type']]]]]}}" class="u-f-ac" bindtap="__e"><view>{{account_typeArr[account_type]}}</view><view class="icon iconfont icon-bi"></view></view></view><view class="user-set-userinfo-list u-f-ac u-f-jsb"><view>时间</view><picker mode="date" value="{{time}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="u-f-ac"><view>{{time}}</view><view class="icon iconfont icon-bi"></view></view></picker></view><view class="user-set-userinfo-list u-f-ac u-f-jsb"><view>备注</view><input class="input1" type="text" data-event-opts="{{[['input',[['__set_model',['','remark','$event',[]]]]]]}}" value="{{remark}}" bindinput="__e"/><view class="icon iconfont icon-bi"></view></view><button class="user-set-btn" type="primary" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">完成</button></scroll-view></swiper-item></swiper></view></view>